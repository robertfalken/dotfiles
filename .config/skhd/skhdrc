hyper - f : yabai -m window --toggle float; sketchybar --trigger yabai_float

hyper - h : yabai -m window --focus west
hyper - j : yabai -m window --focus south
hyper - k : yabai -m window --focus north
hyper - l : yabai -m window --focus east

hyper - z : yabai -m window --toggle zoom-parent; sketchybar --trigger yabai_zoom

hyper - 0x18 : yabai -m window --resize right:100:0 # plus key
hyper - 0x1B : yabai -m window --resize right:-100:0 # minus key

hyper - 0x21 : yabai -m window --swap west # Left bracket ([)
hyper - 0x1E : yabai -m window --swap east # Right bracket (])

hyper - s : yabai -m window --resize abs:1280:720
